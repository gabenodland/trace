{
  "name": "trace-monorepo",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "npm run dev:web",
    "dev:mobile": "cd apps/mobile && npm run dev",
    "dev:web": "cd apps/web && npm run dev",
    "dev:all": "concurrently \"npm run dev:mobile\" \"npm run dev:web\"",
    "build:shared": "cd packages/core && npm run build",
    "build:web": "cd apps/web && npm run build",
    "type-check": "npm run type-check:mobile && npm run type-check:web",
    "type-check:mobile": "cd apps/mobile && npm run type-check",
    "type-check:web": "cd apps/web && npm run type-check",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "test": "cd packages/core && npm test",
    "test:run": "cd packages/core && npm run test:run",
    "android": "expo run:android",
    "ios": "expo run:ios"
  },
  "devDependencies": {
    "@types/uuid": "^10.0.0",
    "@vitest/ui": "^3.2.4",
    "concurrently": "^8.2.2",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "overrides": {
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native-maps": "1.20.1",
    "react-native-svg": "15.12.1",
    "react-native-webview": "13.15.0"
  },
  "dependencies": {
    "@tiptap/extension-underline": "^3.10.7",
    "@tiptap/react": "^3.10.4",
    "@tiptap/starter-kit": "^3.10.4",
    "react-native-gesture-handler": "^2.29.1",
    "react-native-image-viewing": "^0.2.2",
    "react-native-maps": "1.20.1",
    "react-native-reanimated": "^4.1.5",
    "expo": "~54.0.23",
    "react": "19.1.0",
    "react-native": "0.81.5"
  }
}
